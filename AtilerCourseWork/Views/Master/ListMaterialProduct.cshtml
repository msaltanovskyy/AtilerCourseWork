@model IEnumerable<AtilerCourseWork.Models.ProductMaterial>

@{
    ViewData["Title"] = "ListMaterialProduct";
    Layout = null;
}

<link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ProductId)
            </th>
